{
  "name": "http-and-schema",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "publishConfig": {
    "access": "restricted",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js"
    },
    "./*": {
      "require": "./dist/*.js"
    }
  },
  "scripts": {
    "clean": "rimraf build dist",
    "build": "tsc --project tsconfig.build.json",
    "build-watch": "tsc --watch --project tsconfig.build.json",
    "build:tsup": "tsup",
    "build-pkg": "pnpm build && pkg -c package.json dist/index.js",
    "dev": "nodemon",
    "test": "vitest"
  },
  "sideEffects": [],
  "dependencies": {
    "@effect/schema": "0.75.5",
    "@effect/platform-node": "0.73.2",
    "effect": "3.3.1"
  },
  "devDependencies": {
    "@effect/language-service": "0.0.21",
    "@types/chai": "4.3.5",
    "@types/node": "20.5.6",
    "@vitest/coverage-v8": "0.34.2",
    "@vitest/expect": "0.34.2",
    "concurrently": "8.2.1",
    "nodemon": "3.0.1",
    "pkg": "5.8.1",
    "prettier": "3.0.2",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tsup": "8.3.6",
    "typescript": "5.2.2",
    "ultra-runner": "3.10.5",
    "vite": "4.4.9",
    "vitest": "0.34.2"
  },
  "pkg": {
    "targets": [
      "node18-alpine-x64",
      "node18-macos-x64"
    ],
    "outputPath": "build/pkg"
  }
}